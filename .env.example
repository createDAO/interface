# WalletConnect Project ID (required for WalletConnect v2)
# Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# NYKNYC App ID (required for NYKNYC wallet connector)
# Get one at https://nyknyc.app/app/dev/apps
NEXT_PUBLIC_NYKNYC_APP_ID=your_nyknyc_app_id

# DRPC API Key (required for RPC connections)
# Using DRPC for better RPC performance across multiple networks
NEXT_PUBLIC_DRPC_API_KEY=

# NGINX server_name value (e.g. your domain, used in nginx.conf)
NGINX_SERVER_NAME=

# App version and commit hash (set automatically in CI/CD)
NEXT_PUBLIC_APP_VERSION=
NEXT_PUBLIC_COMMIT_HASH=
